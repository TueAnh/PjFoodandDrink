  <tr>   
    <td><%= user.name %></td>
    <td><%= user.address %></td>
    <td><%= user.phone %></td>
    <td><%= user.email %></td>
    <td>
        <%= link_to edit_admin_user_path(user) do %>
          <i class="glyphicon glyphicon-edit"></i>
        <% end %>
        <%= link_to admin_user_path(user), method: :delete,
          data: {confirm: t(".you_sure")} do %>
          <i class="glyphicon glyphicon-trash"></i>
        <% end %>
    </td>
  </tr>
