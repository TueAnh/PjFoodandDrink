<h1>Manage Suggests</h1>
<div class="row">
    <div class="col-lg-12">
        <div class="panel-body">
          <div class="table-responsive">
            <table class="table">
              <thead>
                <tr>
                  <th><%= t ".user" %></th>
                  <th><%= t ".name" %></th>
                  <th><%= t ".price" %></th>
                  <th><%= t ".description" %></th>
                  <th><%= t ".function" %></th>
                </tr>
              </thead>
              <tbody id="list-users">
                <%= render @suggests%>
              </tbody>
            </table>
          </div>
        </div>
      </div>
      <%= link_to "Back", admin_root_path %>
    </div>
</div>