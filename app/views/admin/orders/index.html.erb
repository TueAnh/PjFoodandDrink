<p1>Manage Orders</p1>

<div class="row">
    <div class="col-lg-12">
        <div class="panel-body">
          <div class="table-responsive">
            <table class="table">
              <thead>
                <tr>
                  <th><%= t ".user" %></th>
                  <th><%= t ".date" %></th>
                  <th><%= t ".total" %></th>
                  <th><%= t ".status" %></th>
                  <th><%= t ".function" %></th>
                </tr>
              </thead>
              <tbody id="list-products">
                <%= render @orders%>
              </tbody>
            </table>
          </div>
        </div>
      </div>
      <%= link_to "Back", admin_root_path %>
    </div>
</div>