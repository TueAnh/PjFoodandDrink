  <tr>   
    <td><%= category.name %></td>
    <td><%= category.description %></td>
    <td>
        <%= link_to edit_admin_category_path(category) do %>
          <i class="glyphicon glyphicon-edit"></i>
        <% end %>
        <%= link_to admin_category_path(category), method: :delete do %>
          <i class="glyphicon glyphicon-trash"></i>
        <% end %>
    </td>
  </tr>
